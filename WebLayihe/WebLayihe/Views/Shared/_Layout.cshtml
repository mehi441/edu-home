<!doctype html>
<html class="no-js" lang="en">

<!-- Mirrored from demo.devitems.com/eduhome/index-2.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 21 Jun 2020 19:37:50 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Eduhome - Educational HTML Template</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="apple-touch-icon" href="apple-touch-icon.png">
    <!-- Place favicon.ico in the root directory -->

    <link rel="stylesheet" href="~/Public/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Public/css/animate.css">
    <link rel="stylesheet" href="~/Public/css/meanmenu.css">
    <link rel="stylesheet" href="~/Public/css/magnific-popup.css">
    <link rel="stylesheet" href="~/Public/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Public/css/font-awesome.min.css">

    @*yeni font*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css">


    <link rel="stylesheet" href="~/Public/css/et-line-icon.css">
    <link rel="stylesheet" href="~/Public/css/reset.css">
    <link rel="stylesheet" href="~/Public/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Public/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/Public/css/style.css">
    <link rel="stylesheet" href="~/Public/css/responsive.css">
    <script src="~/Public/js/vendor/modernizr-2.8.3.min.js"></script>
</head>
<body>
    <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <!-- Header Area Start -->
    <header class="top">
        <div class="header-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-12">
                        <div class="header-top-left">
                            <p>HAVE ANY QUESTION ?  @ViewBag.Setting.NavbarPhone </p>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="header-top-right text-right">
                            <ul>
                                @if (Session["User"] == null)
                                {
                                    <li><a href="@Url.Action("Login","Login")">login</a></li>
                                    <li><a href="@Url.Action("Register","Registration")">Registration</a></li>
                                }
                                else
                                {
                                    <li><a href="#">mr/ms: @ViewData["Username"]     </a></li>
                                    <li><a href="@Url.Action("Logout","Login")">Log Out </a></li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-area two header-sticky">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-3 col-xs-6">
                        <div class="logo">
                            <a href="@Url.Action("Index","Home")"><img src="~/Uploads/@ViewBag.Setting.Logo" alt="eduhome" /></a>
                        </div>
                    </div>
                    <div class="col-md-9 col-sm-9 col-xs-6">
                        <div class="content-wrapper text-right">
                            <!-- Main Menu Start -->
                            <div class="main-menu">
                                <nav>
                                    <ul>
                                        <li>
                                            <a href="@Url.Action("Index","Home")">Home</a>
                                        </li>
                                        <li><a href="@Url.Action("Index","About")">About</a></li>
                                        <li>
                                            <a href="@Url.Action("Index","Course")">courses</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Index","Event")">event</a>
                                        </li>
                                        <li class="hidden-sm">
                                            <a href="@Url.Action("Index","Teacher")">teacher</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Index","Blog")">blog</a>
                                        </li>
                                        <li>
                                            <a href="@Url.Action("Index","Contact")">Contact</a>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                            <!-- Main Menu End -->
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="mobile-menu hidden-lg hidden-md hidden-sm"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Header Area End -->

    @RenderBody()

    <!-- Subscribe Start -->
    <div class="subscribe-area pt-60 pb-70">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <div class="subscribe-content section-title text-center">
                        <h2>subscribe our newsletter</h2>
                        <p>I must explain to you how all this mistaken idea </p>
                    </div>
                    <div class="newsletter-form mc_embed_signup">
                        <form  action="@Url.Action("Subscribe","Contact")" method="post" >
                            <div id="mc_embed_signup_scroll" class="mc-form">
                                <input type="email" value="" name="EMAIL" class="email" id="mce-EMAIL" placeholder="Enter your e-mail address" required>
                                <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                                <div class="mc-news" aria-hidden="true"><input type="text" name="b_6bbb9b6f5827bd842d9640c82_05d85f18ef" tabindex="-1" value=""></div>
                                <button id="mc-embedded-subscribe" class="default-btn" type="submit" name="subscribe"><span>subscribe</span></button>
                            </div>
                        </form>
                        <!-- mailchimp-alerts Start -->
                        <div class="mailchimp-alerts">
                            <div class="mailchimp-submitting"></div><!-- mailchimp-submitting end -->
                            <div class="mailchimp-success"></div><!-- mailchimp-success end -->
                            <div class="mailchimp-error"></div><!-- mailchimp-error end -->
                        </div>
                        <!-- mailchimp-alerts end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Subscribe End -->
    <!-- Footer Start -->
    <footer class="footer-area">
        <div class="main-footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-sm-6 col-xs-12">
                        <div class="single-widget pr-60">
                            <div class="footer-logo pb-25">
                                <a href="@Url.Action("Index","Home")"><img src="~/Uploads/@ViewBag.Setting.Logo" alt="eduhome"></a>
                            </div>
                            <p>@ViewBag.Setting.Content</p>
                            <div class="footer-social">
                                
                                <ul>

                                    @foreach (var item in @ViewBag.Setting.SettingSocials)
                                    {
                                        <li><a href="@item.Link"><i class="@item.Icon"></i></a></li>
                                    }
                                </ul>
                                
                                
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12">
                        <div class="single-widget">
                            <h3>information</h3>
                            <ul>
                                <li><a href="@Url.Action("Index","Course")">addmission</a></li>
                                <li><a href="@Url.Action("Index","Course")">Academic Calender</a></li>
                                <li><a href="@Url.Action("Index","Event")">Event List</a></li>
                                <li><a href="@Url.Action("Index","About")">Hostel &amp; Dinning</a></li>
                                <li><a href="@Url.Action("Index","Teacher")">TimeTable</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-2 col-sm-6 col-xs-12">
                        <div class="single-widget">
                            <h3>useful links</h3>
                            <ul>
                                <li><a href="@Url.Action("Index","Course")">our courses</a></li>
                                <li><a href="@Url.Action("Index","About")">about us</a></li>
                                <li><a href="@Url.Action("Index","Teacher")">teachers &amp; faculty</a></li>
                                <li><a href="#">teams &amp; conditions</a></li>
                                <li><a href="@Url.Action("Index","Event")">our events</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-12">
                        <div class="single-widget">
                            <h3>get in touch</h3>
                            <p>@ViewBag.Setting.FistAddress<br>City, Roadno 785 New York</p>
                            <p>@ViewBag.Setting.FirstPhone<br>+880  659  785  658  98</p>
                            <p>@ViewBag.Setting.Email<br>www.eduhome.com</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-bottom text-center">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <p>Copyright © <a href="#" target="_blank">@ViewBag.Setting.Copyright</a> 2017. All Right Reserved By Hastech.</p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- Footer End -->

    <script src="~/Public/js/vendor/jquery-1.12.0.min.js"></script>
    <script src="~/Public/js/bootstrap.min.js"></script>
    <script src="~/Public/js/jquery.meanmenu.js"></script>
    <script src="~/Public/js/jquery.magnific-popup.js"></script>
    <script src="~/Public/js/ajax-mail.js"></script>
    <script src="~/Public/js/owl.carousel.min.js"></script>
    <script src="~/Public/js/jquery.mb.YTPlayer.js"></script>
    <script src="~/Public/js/jquery.nicescroll.min.js"></script>
    <script src="~/Public/js/plugins.js"></script>
    <script src="~/Public/js/main.js"></script>
</body>

